    "features": [
        {
            "id": 1,
            "name": "Header",
            "key": "header",
            "shortName": "Header",
            "description": "Der Header ist eine Navigation für den Nutzer und kann ihn z.B. über featured und action Links durch die Seite leiten.",
            "jsonSchema": {
                "type": "object",
                "properties": {
                    "logoUrl": {
                        "type": ["string", "null"],
                        "title": "Logo-Link-URL (falls abweichend von Startseite)",
                        "format": "uri-reference"
                    },
                    "actionLinks": {
                        "type": "array",
                        "items": {
                            "$ref": "#/definitions/linkElement"
                        },
                        "title": "Actions"
                    },
                    "featuredLinks": {
                        "type": "array",
                        "items": {
                            "$ref": "#/definitions/linkElement"
                        },
                        "title": "Featured"
                    }
                },
                "definitions": {
                    "linkElement": {
                        "type": "object",
                        "allOf": [
                            {
                                "if": {
                                    "properties": {
                                        "elementType": {
                                            "const": "TEXT_LINK"
                                        }
                                    }
                                },
                                "then": {
                                    "properties": {
                                        "linkName": {
                                            "type": "string",
                                            "title": "Name"
                                        },
                                        "modifierClassExtension": {
                                            "type": "string",
                                            "title": "Link hervorheben (CSS Klasse)"
                                        }
                                    }
                                }
                            },
                            {
                                "if": {
                                    "not": {
                                        "properties": {
                                            "elementType": {
                                                "const": "USER_ID_LINK"
                                            }
                                        }
                                    }
                                },
                                "then": {
                                    "properties": {
                                        "url": {
                                            "type": ["string", "null"],
                                            "title": "URL",
                                            "format": "uri-reference"
                                        }
                                    }
                                }
                            }
                        ],
                        "properties": {
                            "elementType": {
                                "enum": [
                                    "TEXT_LINK",
                                    "test_MEDIA_LINK",
                                    "NAVIGATION_TRIGGER",
                                    "PNA_LINK",
                                    "SEARCH_LINK",
                                    "USER_PROFILE_LINK",
                                    "USER_ID_LINK",
                                    "WEATHER_LINK",
                                    "APP_LINK"
                                ],
                                "type": "string",
                                "title": "Element",
                                "default": "TEXT_LINK",
                                "enumNames": [
                                    "Textlink",
                                    "test Media Icon",
                                    "Burger Menu Icon",
                                    "PNA Icon (nur mobile)",
                                    "Suche Icon",
                                    "User Icon",
                                    "USER.ID Icon",
                                    "Wetter Icon",
                                    "App Link (nur mobile)"
                                ]
                            }
                        }
                    }
                },
                "additionalProperties": false
            },
            "created": "2024-03-15T09:28:28Z",
            "modified": "2024-03-15T09:28:28Z"
        },
        {
            "id": 2,
            "name": "Footer",
            "key": "footer",
            "shortName": "Footer",
            "description": "Im Footer können weitere Links zu wichtigen Seiten gesetzt werden.",
            "jsonSchema": {
                "type": "object",
                "properties": {
                    "links": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "url": {
                                    "type": "string",
                                    "title": "URL",
                                    "format": "uri-reference"
                                },
                                "name": {
                                    "type": "string",
                                    "title": "Name"
                                }
                            }
                        },
                        "title": "Links"
                    }
                },
                "additionalProperties": false
            },
            "created": "2024-03-15T09:28:28Z",
            "modified": "2024-03-15T09:28:28Z"
        }
    ],
