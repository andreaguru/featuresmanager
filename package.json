{
  "name": "settings-ui",
  "version": "1.6.6",
  "description": "Dashboard to manage settings API based on NextJs",
  "private": true,
  "scripts": {
    "server": "cd json-db && json-server db.json --port 3004 --middlewares file.js",
    "dev": "concurrently \"npm run server\" \"cross-env ENV_FILE=envs/.env.development next dev\"",
    "dev:staging": "cross-env ENV_FILE=envs/.env.staging next dev",
    "build:development": "cross-env ENV_FILE=envs/.env.development next build",
    "start:development": "cross-env ENV_FILE=envs/.env.development next start",
    "build:staging": "ENV_FILE=envs/.env.staging next build",
    "start:staging": "ENV_FILE=envs/.env.staging next start",
    "build:production": "ENV_FILE=envs/.env.production next build",
    "start:production": "ENV_FILE=envs/.env.production next start",
    "eslint": "eslint \"./**/*.{ts,tsx}\"",
    "eslint:fix": "eslint \"./**/*.{ts,tsx}\" --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "npx prettier --write . ",
    "stylelint": "stylelint \"./**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"./**/*.{css,scss}\" --fix",
    "lint:fix-all": "eslint \"./**/*.{ts,tsx}\" --fix && npx prettier --write . && stylelint \"./**/*.{css,scss}\" --fix",
    "test": "jest",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.14",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@mui/system": "^5.16.7",
    "@mui/x-data-grid": "^6.20.4",
    "@rjsf/core": "^5.20.0",
    "@rjsf/mui": "^5.20.0",
    "@rjsf/utils": "^5.20.0",
    "@rjsf/validator-ajv8": "^5.20.0",
    "@types/jest": "^29.5.12",
    "dotenv": "^16.4.5",
    "next": "^13.5.6",
    "npm-check-updates": "^17.0.6",
    "pino": "^9.3.2",
    "pino-http": "^10.2.0",
    "pino-pretty": "^11.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.13.0",
    "sharp": "^0.33.4",
    "swr": "^2.2.5",
    "webpack-strip-code": "^1.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^14.3.1",
    "@types/react": "^18.3.3",
    "@types/testing-library__react": "^10.0.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-server": "^0.17.4",
    "next-router-mock": "^0.9.13",
    "prettier": "^3.3.3",
    "stylelint": "^16.8.2",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.4.5"
  }
}
